---
import Stat from "./Stat.astro";
import heroSvg from "../../assets/hero.svg";

const stats = [
  { category: "Customers", amount: "5,000+" },
  { category: "Deliveries", amount: "8,000+" },
  { category: "Ratings", amount: "1,000+" },
] as const;
---

<section
  id="hero"
  class="hero"
>
  <div class="hero__left">
    <div class="hero__claim">
      <p>#1 Best Healthy Salad</p>
    </div>
    <h1 class="hero__title">
      <span>Skip</span> The Diet, <br /> Just Eat Healthy <br /> With Jez Salad
    </h1>
    <p class="hero__sub-text">
      Imagine you don't need a diet because we provide healthy and delicious
      food for you!
    </p>
    <button class="hero__cta">Order Food</button>
    <div class="hero__stats">
      {
        stats.map((stat) => (
          <Stat
            category={stat.category}
            amount={stat.amount}
          />
        ))
      }
    </div>
  </div>
