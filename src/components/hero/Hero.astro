<style lang="scss">
  .hero {
    margin-top: 8rem;

    padding: 0 1rem 4rem 1rem;

    @media screen and (min-width: 640px) {
      display: flex;
      align-items: center;
      justify-content: space-between;
    }

    @media screen and (min-width: 1024px) {
      padding: 0 5rem 8rem 5rem;
    }

    @media screen and (min-width: 1280px) {
      padding: 0 8rem 8rem 8rem;
    }

    @media screen and (min-width: 1440px) {
      padding: 0 13rem 8rem 13rem;
    }

    &__left {
      @media screen and (min-width: 640px) {
        flex-basis: 50%;
      }

      @media screen and (min-width: 1440px) {
        flex-basis: 60%;
      }
    }

    &__claim {
      max-width: 215px;

      padding: 0.5rem 0;

      border-radius: 10rem;

      color: #fb9333;
      background-color: #fff0dc;
      font-size: 1.6rem;
      text-align: center;
    }

    &__title {
      font-size: clamp(4rem, 5vw, 7.2rem);
      color: #575757;
    }

    &__title span {
      color: #fb9333;
    }

    &__sub-text {
      margin: 3rem 0;
      width: 80%;

      font-size: clamp(1.6rem, 2vw, 2rem);
      color: #828282;
    }

    &__cta {
      padding: 1.5rem 3rem;
      max-width: 150px;

      border: none;
      border-radius: 100px;

      background-color: #fb9333;
      color: #fff;
      font-size: 1.6rem;

      cursor: pointer;
    }

    &__stats {
      margin-top: 4rem;

      @media screen and (min-width: 640px) {
        display: flex;
      }

      @media screen and (min-width: 1280px) {
        margin-top: 8rem;
        gap: 3rem;
      }
    }

    &__right {
      width: 70%;
      margin: -40% 0 0 auto;

      @media screen and (min-width: 640px) {
        margin: 0;
        width: 50%;
      }
    }

    &__img {
      width: 100%;
      max-height: 75vh;
    }
  }

  .hidden {
    display: none;
  }
